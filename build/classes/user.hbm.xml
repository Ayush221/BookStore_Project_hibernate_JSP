<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>  
 <class name="com.User.User" table="user">  
 
<id name="number" type="long"><column name="mobile_num" /><generator class="assigned"></generator>  </id> 
 <property name="pwd" type="string"></property>  
 <property name="email" type="string"></property>  
 <property name="address" type="string"></property>
 <property name="city" type="string"></property>
 <property name="pincode" type="int"></property>
 </class>  
  </hibernate-mapping>  
 